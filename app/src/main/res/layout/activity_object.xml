<?xml version="1.0" encoding="utf-8"?>



<RelativeLayout
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ObjectActivity">

    <ImageView
        android:id="@+id/bed"
        android:layout_width="123dp"
        android:layout_height="117dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/bed" />

    <ImageView

        android:id="@+id/computer"
        android:layout_width="118dp"
        android:layout_height="114dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/computer" />

    <ImageView
        android:id="@+id/couch"

        android:layout_width="122dp"
        android:layout_height="117dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:src="@drawable/couch" />

    <ImageView

        android:id="@+id/rchair"
        android:layout_width="126dp"
        android:layout_height="117dp"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:src="@drawable/rchair" />

    <ImageView
        android:id="@+id/laptop"
        android:layout_width="129dp"
        android:layout_height="117dp"
        android:layout_centerInParent="true"
        android:src="@drawable/laptop" />

    <ImageView
        android:id="@+id/lamp"
        android:layout_width="118dp"
        android:layout_height="117dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:src="@drawable/lamp" />

    <ImageView
        android:id="@+id/tablelamp"
        android:layout_width="129dp"
        android:layout_height="126dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:src="@drawable/tablelamp" />

    <ImageView
        android:id="@+id/wmachine"
        android:layout_width="129dp"
        android:layout_height="123dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/wmachine" />



</RelativeLayout>